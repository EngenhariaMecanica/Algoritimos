#include <stdio.h>

int main(void) {
  float n1, n2, n3, n4, media;

  printf("Digite a primeira nota: ");
  scanf("%f", &n1);
  
  printf("Digite a segunda nota: ");
  scanf("%f", &n2);

  printf("Digite a terceira nota: ");
  scanf("%f", &n3);

  printf("Digite a quarta nota: ");
  scanf("%f", &n4);

  media = (n1 + n2 +n3+ n4)/4;
  
  if(media>=0 && media<=4){
    printf("Reprovado");
  } else
  if (media>4 && media<7){
    printf("Prova final");
  } else
  if (media>=7 && media <=10){
    printf("Aprovado");
  }
  
  return 0;
}
